#.subsection Rune

Show #.econ run 100
    BaseType == "Time Rune" "Power Rune"
	#.import < < styles > currency -> 100+c

Show #.econ run 10 100
    BaseType == "Mountain Rune"
	#.import < < styles > currency -> 10_100c

Show #.econ run 2 10
    BaseType == "River Rune" "Journey Rune"
    #.import < < styles > currency -> 1_10c

Show #.econ run 0 2
    BaseType == "Bounty Rune" "Sun Rune" "Bound Rune" "Life Rune" "War Rune"
	#.import < < styles > currency -> 1-c fine