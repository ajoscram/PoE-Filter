Show
    ShapedMap True
    BaseType "Channel Map"
    Class "Maps"
	SetTextColor 0 0 0
    SetBackgroundColor 30 155 255
    SetBorderColor 0 0 0
    SetFontSize 40
	MinimapIcon 2 Blue Hexagon
	PlayEffect Blue

#Tier 16 Maps
Show
    MapTier = 16
	Class "Maps"
	SetTextColor 0 0 0
    SetBackgroundColor 255 0 0
    SetBorderColor 0 0 0
    SetFontSize 40
	PlayAlertSoundPositional 2 100	
	MinimapIcon 0 Red Hexagon
	PlayEffect Red

#Shaped Red Maps
Show
    ShapedMap True
    MapTier >= 11
	Class "Maps"
	SetTextColor 0 0 0
    SetBackgroundColor 240 80 80
    SetBorderColor 0 0 0
	SetFontSize 40
	MinimapIcon 1 Red Hexagon
	PlayEffect Red Temp

#Shaped Yellow Maps
