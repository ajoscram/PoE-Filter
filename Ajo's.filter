    MapTier >= 6
	Class "Maps"
	SetTextColor 0 0 0
    SetBackgroundColor 255 255 119
    SetBorderColor 0 0 0
	SetFontSize 40
	MinimapIcon 1 Yellow Hexagon
	PlayEffect Yellow Temp

